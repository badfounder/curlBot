<div class="form-group row">
  <label class="col-form-label col-sm-2" for="teamID">Team Name</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" type="text" name="teamID"  step="0.01" value="<%= model.teamID%>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="win">Wins</label>
  <div class="col-sm-7">
    <input class="form-control" type="number" name="win"  step="0.01" value="<%= model.win %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="lose">Lose</label>
  <div class="col-sm-7">
    <input class="form-control" name="lose" type="number"  step="0.01"value="<%= model.lose %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="ptsFor">Points For</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" maxlength="4" step="0.01" type="number" name="ptsFor" value="<%= model.ptsFor%>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="ptsAgainst">Points Against</label>
  <div class="col-sm-7">
    <input class="form-control" type="number" name="ptsAgainst"  step="0.01" value="<%= model.ptsAgainst %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="hamEff">Hammer Efficiency</label>
  <div class="col-sm-7">
    <input class="form-control" name="hamEff" type="number" step="0.01" value="<%= model.hamEff %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="stlDef">Steal Defense</label>
  <div class="col-sm-7">
    <input class="form-control" name="stlDef" type="number"  step="0.01" value="<%= model.stlDef %>" />
  </div>
</div>




