<div class="form-group row">
  <label class="col-form-label col-sm-2" for="teamID">Team Name</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" name="teamID" value="<%= model.teamID%>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="win">Wins</label>
  <div class="col-sm-7">
    <input class="form-control" name="win" value="<%= model.win %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="lose">Lose</label>
  <div class="col-sm-7">
    <input class="form-control" name="lose" value="<%= model.lose %>" />
  </div>
</div>
